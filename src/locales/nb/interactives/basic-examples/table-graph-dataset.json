{
  "title": "Table, Graph and DataSet Demo",
  "publicationStatus": "public",
  "subtitle": "",
  "aspectRatio": 0.6,
  "models": [
    {
      "type": "md2d",
      "id": "simple-gas$0",
      "url": "models-converted/lab-version/1/md2d/new-examples-for-nextgen/simple-gas$0.json",
      "viewOptions": {
        "controlButtons": "play_reset"
      }
    }
  ],
  "dataSets": [
    {
      "name": "table-dataset",
      "streamDataFromModel": false,
      "clearOnModelReset": false,
      "properties": [
        "time",
        "Editable col"
      ],
      "initialData": {
        "time": [0, 0],
        "Editable col": ["abc", "xyz"]
      }
    },
    {
      "name": "graph-dataset",
      "properties": [
        "displayTime",
        "totalEnergy"
      ],
      "serializableProperties": "none"
    },
    {
      "name": "table-graph-dataset",
      "properties": [
        "displayTime",
        "totalEnergy",
        "X coord",
        "Y1 coord",
        "Y2 coord"
      ],
      "serializableProperties": [
        "X coord",
        "Y1 coord",
        "Y2 coord"
      ],
      "streamDataFromModel": true,
      "clearOnModelReset": false
    }
  ],
  "components": [
    {
      "type": "text",
      "id": "label1",
      "text": "Left table is editable, right not. Both use the same data set, so they are synchronized:"
    },
    {
      "type": "button",
      "id": "clear-dataset",
      "text": "Clear tables",
      "action": "clearDataSet('table-dataset');"
    },
    {
      "type": "button",
      "id": "reset-dataset",
      "text": "Reset tables",
      "action": "resetDataSet('table-dataset');"
    },
    {
      "id": "table1",
      "type": "table",
      "dataSet": "table-dataset",
      "title": null,
      "addNewRows": true,
      "showBlankRow": true,
      "indexColumn": true,
      "height": "10em",
      "propertyColumns": [
        {
          "name": "time",
          "format": "r"
        },
        {
          "name": "Editable col",
          "format": "r"
        }
      ]
    },
    {
      "id": "table2",
      "type": "table",
      "dataSet": "table-dataset",
      "title": null,
      "addNewRows": true,
      "showBlankRow": false,
      "indexColumn": true,
      "height": "10em",
      "propertyColumns": [
        {
          "name": "time",
          "format": "r",
          "editable": false
        },
        {
          "name": "Editable col",
          "format": "r",
          "editable": false
        }
      ]
    },
    {
      "type": "text",
      "id": "label2",
      "text": "Table uses the same data set as graph, so they are synchronized:"
    },
    {
      "id": "graph1",
      "type": "graph",
      "dataSet": "graph-dataset",
      "ymin": -100,
      "ymax": 100,
      "xmax": 10,
      "xTickCount": 5,
      "yTickCount": 6,
      "xFormatter": ".2r",
      "yFormatter": ".2s",
      "properties": [
        "totalEnergy"
      ],
      "width": "12em",
      "height": "15em"
    },
    {
      "id": "table3",
      "type": "table",
      "dataSet": "graph-dataset",
      "title": null,
      "addNewRows": true,
      "showBlankRow": false,
      "indexColumn": true,
      "height": "15em",
      "propertyColumns": [
        "displayTime",
        "totalEnergy"
      ]
    },
    {
      "type": "text",
      "id": "label3",
      "text": "Table and graph are synchronized, use table to plot data on the graph:"
    },
    {
      "id": "graph2",
      "type": "graph",
      "dataSet": "table-graph-dataset",
      "ymin": -10,
      "ymax": 10,
      "xmax": 10,
      "xTickCount": 5,
      "yTickCount": 5,
      "xFormatter": ".2r",
      "yFormatter": ".2s",
      "xProperty": [
        "displayTime",
        "X coord",
        "X coord"
      ],
      "properties": [
        "totalEnergy",
        "Y1 coord",
        "Y2 coord"
      ],
      "width": "20em",
      "height": "15em"
    },
    {
      "id": "table4",
      "type": "table",
      "dataSet": "table-graph-dataset",
      "title": null,
      "addNewRows": true,
      "showBlankRow": true,
      "indexColumn": true,
      "height": "15em",
      "width": "15em",
      "propertyColumns": [
        {
          "name": "X coord",
          "format": "r"
        },
        {
          "name": "Y1 coord",
          "format": "r"
        },
        {
          "name": "Y2 coord",
          "format": "r"
        }
      ]
    }
  ],
  "layout": {
    "bottom": [
      ["label1"],
      ["clear-dataset", "reset-dataset"],
      ["table1", "table2"],
      ["label2"],
      ["graph1", "table3"],
      ["label3"],
      ["graph2", "table4"]
    ]
  }
}