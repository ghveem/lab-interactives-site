{
  "title": "Outputs",
  "publicationStatus": "public",
  "subtitle": "Use outputs and Scripting API to graph data.",
  "models": [
    {
      "type": "energy2d",
      "id": "compare-capacity",
      "url": "models-converted/lab-version/1/energy2d/compare-capacity.json",
      "importedFrom": "imports/energy2d/models-xml/compare-capacity.e2d",
      "viewOptions": {
        "controlButtons": "play_reset"
      }
    }
  ],
  "outputs": [
    {
      "name": "leftPartTemp",
      "value": "return getTemperatureAt(2.5, 5);"
    },
    {
      "name": "rightPartTemp",
      "value": "return getTemperatureAt(7.5, 5);"
    }
  ],
  "exports": {
    "perRun": [
    ],
    "perTick": [
      "leftPartTemp",
      "rightPartTemp"
    ]
  },
  "components": [
    {
      "type": "barGraph",
      "id": "bar-graph-left",
      "min": 0,
      "max": 50,
      "title": "Left Obstacle Temperature (°C)",
      "property": "leftPartTemp"
    },
    {
      "type": "barGraph",
      "id": "bar-graph-right",
      "min": 0,
      "max": 50,
      "title": "Right Obstacle Temperature (°C)",
      "property": "rightPartTemp"
    }
  ],
  "template": [
      {
      "id": "left",
      "top": "model.top",
      "right": "model.left",
      "height": "model.height",
      "padding-left": "0.5em",
      "padding-right": "0.5em"
    },
    {
      "id": "right",
      "top": "model.top",
      "left": "model.right",
      "height": "model.height",
      "padding-left": "0.5em",
      "padding-right": "0.5em"
    }
  ],
  "layout": {
    "left": ["bar-graph-left"],
    "right": ["bar-graph-right"]
  }
}