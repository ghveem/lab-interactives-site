{
  "title": "Air Pollution 1a",
  "publicationStatus": "public",
  "subtitle": "Wind, Geography, and Pollution Sources (no graph)",
  "helpOnLoad": false,
  "aboutOnLoad": true,
  "about": [
    "Explore some factors that affect the pollution levels over a city.",
    "",
    "Change the direction and speed of the wind, the number of cars, and the number of power plants ",
    "to see how it affects the level of pollutants over the city.",
    ""
  ],
  "credits": ["This [High-Adventure Science](http://has.concord.org \"High-Adventure Science\") interactive ",
              "was developed under a series of grants from the [National Science Foundation](http://nsf.gov \"National Science Foundation\") ",
              "(DRL-0929774, DRL-1220756 ) in partnership with the ",
              "[National Geographic Society](http://nationalgeographic.org/education/ \"National Geographic Society\")."],
  "theme": ["has","standalone-playback","no-slider-bar"],
  "aspectRatio": 0.98,
  "fontScale": 0.9,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions":{
        "url": "//models-resources.concord.org/agentscript/air-pollution/lab-embeddable.html",
        "includeSunlight": false,
        "windSpeed": 0,
        "numCars": 2,
        "numFactories": 1
      },
      "viewOptions": {
        "controlButtons": "play_reset"
      },
      "onLoad": []
    }
  ],
  "parameters": [
  ],
  "outputs": [
    {
      "name": "ticks",
      "value": "return iframeOutput('ticks');"
    },
    {
      "name": "primaryAQI",
      "value": "return iframeOutput('primaryAQI');"
    },
    {
      "name": "secondaryAQI",
      "value": "return iframeOutput('secondaryAQI');"
    }
  ],
  "components": [
    {
      "id": "windSlider",
      "type": "slider",
      "min": -100,
      "max": 100,
      "steps": 10,
      "title": "Wind",
       "fillColor": "#459ACE",
      "fillToValue": 0,
      "labels": [
        {
          "value": "-100",
          "label": "<i class='icon-arrow-left'></i>"
        },
        {
          "value": 0,
          "label": "None"
        },
        {
          "value": "100",
          "label": "<i class='icon-arrow-right'></i>"
        }
      ],
      "width": "25em",
      "height": "auto",
      "property": "windSpeed",
      "retainProperty": true,
      "disabled": false,
      "tooltip": "Change the speed and direction of the wind."
    },
    {
      "id": "carsSlider",
      "type": "slider",
      "min": 0,
      "max": 10,
      "steps": 10,
      "title": "Cars",
      "fillColor": "#D84748",
      "labels": [
        {
          "value": "0",
          "label": "<i class='icon-minus'></i>"
        },
        {
          "value": "10",
          "label": "<i class='icon-plus'></i>"
        }
      ],
      "width": "25em",
      "height": "auto",
      "property": "numCars",
      "retainProperty": true,
      "disabled": false,
      "tooltip": "Change the number of cars."
    },
    {
      "id": "factoriesSlider",
      "type": "slider",
      "min": 0,
      "max": 5,
      "steps": 5,
      "title": "Power Plants",
      "fillColor": "#E67E22",
      "labels": [
        {
          "value": "0",
          "label": "<i class='icon-minus'></i>"
        },
        {
          "value": "5",
          "label": "<i class='icon-plus'></i>"
        }
      ],
      "width": "25em",
      "height": "auto",
      "property": "numFactories",
      "retainProperty": true,
      "disabled": false,
      "tooltip": "Change the number of power plants."
    },
    {
      "id": "playback",
      "type": "playback",
      "stepping": true
    }
  ],
   "layout": {
    "playback-container": [
      "playback"
    ],
    "bottomLeft": [
      [
        "windSlider"
      ],
      [
        "carsSlider"
      ],
      [
        "factoriesSlider"
      ]
    ]
  },
  "template": [
    {
      "id": "playback-container",
      "top": "model.bottom",
      "left": "model.left"
    
    },
    
    {
      "id": "bottomLeft",
      "top": "playback-container.bottom",
      "left": "model.left"
    }
  ],
  "i18nMetadata": "locales/metadata/interactives/air-pollution/air-pollution-1a.json"
}
