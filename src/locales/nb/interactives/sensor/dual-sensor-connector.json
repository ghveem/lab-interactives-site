{
  "title": "Sensor Connector (dual)",
  "publicationStatus": "draft",
  "labEnvironment": "development",
  "subtitle": "Collect data from two sensors wirelessly from the locally running Sensor Connector.",
  "aspectRatio": 1.3,
  "helpOnLoad": false,
  "about": "",
  "fontScale": 0.9,
  "models": [
    {
      "type": "dual-sensor-connector",
      "id": "signal1",
      "model": {},
      "viewOptions": {
        "controlButtons": "play_reset",
        "controlButtonStyle": "text"
      },
      "modelOptions": {
        "useDuration": true
      },
      "onLoad": [
        "function setGraphRange() {",
        "  syncAxisRangesToPropertyRanges('propertiesGraph');",
        "  resetGraphSelection('propertiesGraph');",
        "}",
        "function setGraphRange2() {",
        "  syncAxisRangesToPropertyRanges('propertiesGraph2');",
        "  resetGraphSelection('propertiesGraph2');",
        "}",
        "function setGraphRanges() {",
        "  setGraphRange();",
        "  setGraphRange2();",
        "}",
        "onPropertyChange('actualDuration', setGraphRanges);",
        "setGraphRanges();"
      ]
    }
  ],
  "components": [
    {
      "type": "graph",
      "id": "propertiesGraph",
      "title": " ",
      "xlabel": "Time (s)",
      "ymin": 0,
      "ymax": 6,
      "xmax": 30,
      "xTickCount": 6,
      "xFormatter": ".2s",
      "yFormatter": ".2s",
      "xProperty": "displayTime",
      "properties": [
        "sensorReading"
      ],
      "enableSelectionButton": true,
      "clearSelectionOnLeavingSelectMode": true,
      "resetAxesOnReset": false
    },
    {
      "type": "graph",
      "id": "propertiesGraph2",
      "title": " ",
      "xlabel": "Time (s)",
      "ymin": 0,
      "ymax": 6,
      "xmax": 30,
      "xTickCount": 6,
      "xFormatter": ".2s",
      "yFormatter": ".2s",
      "xProperty": "displayTime",
      "properties": [
        "sensorReading2"
      ],
      "enableSelectionButton": true,
      "clearSelectionOnLeavingSelectMode": true,
      "resetAxesOnReset": false
    }
  ],
  "layout": {
    "sensor1-graph": [
      "propertiesGraph"
    ],
    "sensor2-graph": [
      "propertiesGraph2"
    ]
  },
  "template": [
    {
      "id": "sensor1-graph",
      "top": "model.bottom * 0.7",
      "width": "interactive.width",
      "height": "(interactive.height - model.height) * 0.5",
      "padding-right": "1em"
    },
    {
      "id": "sensor2-graph",
      "top": "sensor1-graph.bottom * 0.95",
      "width": "interactive.width",
      "height": "(interactive.height - model.height) * 0.5",
      "padding-right": "1em"
    }
  ],
  "logging": {
    "properties": [
      "sensorName",
      "sensorName2"
    ]
  },
  "exports": {
    "selectionComponents": [
      "propertiesGraph",
      "propertiesGraph2"
    ],
    "perRun": [
      "sensorName",
      "sensorName2"
    ],
    "perTick": [
      "sensorReading",
      "sensorReading2"
    ]
  }
}