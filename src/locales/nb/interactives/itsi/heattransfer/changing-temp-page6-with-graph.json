  "outputs": [
    {
      "name": "rightAtomsTemp",
      "label": "Temperature of Molecules on Right",
      "unitAbbreviation": "°C",
      "value": [
        "return 0.1 * getTemperatureOfAtoms(get('rightAtomsIndices'));"
      ]      
    },
    {
      "name": "leftAtomsTemp",
