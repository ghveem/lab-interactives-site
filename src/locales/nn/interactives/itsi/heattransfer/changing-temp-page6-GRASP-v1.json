    },
    {
      "type": "checkbox",
      "id": "show-ke",
      "text": "K. E. Shading",
      "property": "keShading"
    },
    {
      "type": "text",
      "id": "instructions",
      "text": "Choose a material to fill the space between the two solids."
    },
    {
      "type": "radio",
      "id": "select-model-buttons",
      "orientation": "horizontal",
      "options": [
        {
          "text": "Vacuum",
