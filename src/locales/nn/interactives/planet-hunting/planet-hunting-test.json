{
  "title": "Planet Hunting (test)",
  "publicationStatus": "public",
  "credits": ["This [High-Adventure Science](http://has.concord.org \"High-Adventure Science\") interactive ",
              "was developed under a series of grants from the [National Science Foundation](http://nsf.gov \"National Science Foundation\") ",
              "(DRL-0929774, DRL-1220756 ) in partnership with the ",
              "[National Geographic Society](http://nationalgeographic.org/education/ \"National Geographic Society\")."],
  "theme": [
    "has",
    "standalone-playback",
    "no-slider-bar",
    "graph-title-bg"
  ],
  "aspectRatio": 0.82,
  "fontScale": 0.7,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions": {
        "url": "//concord-consortium.github.io/space-models/planet-hunting/",
        "timestep": 0.001,
        "planet.diameter": 1,
        "planet.rocky": true,
        "telescope.precision": 100,
        "habitationZone.visible": true,
        "camera.tiltLocked": false
      },
      "viewOptions": {
        "controlButtons": "play_reset"
      },
      "onLoad": [
      ]
    }
  ],
  "outputs": [
    {
      "name": "time",
      "value": "return iframeOutput('time') * 365 || 0;"
    },
    {
      "name": "starCamVelocity",
      "value": "return iframeOutput('telescope.starCamVelocity') || 0;"
    },
    {
      "name": "lightIntensity",
      "value": "return iframeOutput('telescope.lightIntensity') || 0;"
    },
    {
      "name": "planet.mass",
      "value": "return iframeOutput('planet.mass') || 0;"
    },
    {
      "name": "camera.tilt",
      "value": "return iframeOutput('camera.tilt') || 0;"
    },
    {
      "name": "habitability.starType",
      "value": "return iframeOutput('habitability.starType');"
    },
    {
      "name": "habitability.planetType",
      "value": "return iframeOutput('habitability.planetType');"
    },
    {
      "name": "habitability.planetSize",
      "value": "return iframeOutput('habitability.planetSize');"
    },
    {
      "name": "habitability.orbitalDistance",
      "value": "return iframeOutput('habitability.orbitalDistance');"
    }
  ],
  "parameters": [
    {
      "name": "presetPlanet",
      "initialValue": "Earth",
      "onChange": [
        "post('loadPreset', value);",
        "post('makeCircularOrbit');"
      ]
    },
    {
      "name": "presetStar",
      "initialValue": "Sun",
      "onChange": [
        "post('loadPreset', value);",
        "post('makeCircularOrbit');"
      ]
    },
    {
      "name": "speed",
      "initialValue": 50,
      "onChange": [
        "var v = value / 50;",
        "if (v > 1) v = Math.pow(v, 4);",
        "if (v < 1) v = Math.sqrt(v);",
        "set('timestep', 0.001 * v);"
      ]
    }
  ],
  "components": [
    {
      "id": "circular-orbit",
      "type": "button",
      "text": "Make circular orbit",
      "action": "post('makeCircularOrbit');"
    },
    {
      "id": "habitability-label",
      "type": "text",
      "text": "**Habitability**"
    },
    {
      "id": "analyze",
      "type": "button",
      "text": "Analyze",
      "action": "post('analyzeHabitability');"
    },
    {
      "id": "clear-graphs",
      "type": "button",
      "text": "Clear all graphs",
      "action": [
        "var newXMin = Math.round(get('time'));",
        "var newXMax = newXMin + 600;",
        "setComponentAttributes('star-cam-velocity', {xmin: newXMin, xmax: newXMax});",
        "setComponentAttributes('light-intensity', {xmin: newXMin, xmax: newXMax});"
      ]
    },
    {
      "id": "timestep",
      "type": "slider",
      "fillColor": "#2DA343",
      "min": 1,
      "max": 100,
      "steps": 100,
      "title": "Speed",
      "labels": [
        {
          "value": 1,
          "label": "slow"
        },
        {
          "value": 100,
          "label": "fast"
        }
      ],
      "property": "speed"
    },
    {
      "id": "planet-diameter",
      "type": "slider",
      "fillColor": "#459ACE",
      "min": 1,
      "max": 50,
      "steps": 50,
      "title": "Planet diameter",
      "labels": [
        {
          "value": 1,
          "label": "1x Earth"
        },
        {
          "value": 50,
          "label": "50x Earth"
        }
      ],
      "property": "planet.diameter"
    },
    {
      "id": "telescope-precision",
      "type": "slider",
      "fillColor": "#459ACE",
      "min": 5,
      "max": 100,
      "steps": 100,
      "title": "Telescope precision",
      "labels": [
        {
          "value": 5,
          "label": "5%"
        },
        {
          "value": 100,
          "label": "100%"
        }
      ],
      "property": "telescope.precision"
    },
    {
      "id": "planet-mass",
      "type": "numericOutput",
      "property": "planet.mass",
      "label": "Planet mass:",
      "displayValue": "return (value < 1 ? value.toFixed(1) : value.toFixed(0)) + ' x Earth';"
    },
    {
      "id": "planet-rocky",
      "type": "radio",
      "orientation": "horizontal",
      "label": "Planet type",
      "labelOn": "left",
      "property": "planet.rocky",
      "options": [
        {
          "value": false,
          "text": "Gaseous"
        },
        {
          "value": true,
          "text": "Rocky"
        }
      ]
    },
    {
      "id": "preset-planet",
      "type": "pulldown",
      "label": "Preset planets",
      "labelOn": "left",
      "property": "presetPlanet",
      "options": [
        {
          "text": "Earth",
          "value": "Earth"
        },
        {
          "text": "Mars",
          "value": "Mars"
        },
        {
          "text": "Jupiter",
          "value": "Jupiter"
        },
        {
          "text": "Venus",
          "value": "Venus"
        },
        {
          "text": "Like Mercury but 10x larger diameter",
          "value": "MercuryDiameter10x"
        },
        {
          "text": "Like Earth but 10x larger diameter",
          "value": "EarthDiameter10x"
        },
        {
          "text": "Earth-like planet near the star",
          "value": "EarthNearTheStar"
        },
        {
          "text": "Large rocky planet very near the star",
          "value": "LargeNearTheStar"
        },
        {
          "text": "No planet",
          "value": "NoPlanet"
        },
        {
          "text": "Custom",
          "value": ""
        }
      ]
    },
    {
      "id": "habitation-zone-vis",
      "type": "checkbox",
      "text": "Habitation zone",
      "property": "habitationZone.visible"
    },
    {
      "id": "tilt-locked",
      "type": "checkbox",
      "text": "Tilt locked",
      "property": "camera.tiltLocked"
    },
    {
      "id": "preset-star",
      "type": "pulldown",
      "label": "Preset stars",
      "labelOn": "left",
      "property": "presetStar",
      "options": [
        {
          "text": "M - red (temp 3120 K)",
          "value": "StarRed"
        },
        {
          "text": "K - orange (temp 4640 K)",
          "value": "StarOrange"
        },
        {
          "text": "G - yellow (temp 5920 K) (Sun)",
          "value": "Sun"
        },
        {
          "text": "F - white (temp 7240 K)",
          "value": "StarWhite"
        },
        {
          "text": "A - blue (temp 8620 K)",
          "value": "StarBlue"
        }
      ]
    },
    {
      "id": "star-cam-velocity",
      "type": "graph",
      "enableAxisScaling": false,
      "enableZooming": false,
      "properties": [
        "starCamVelocity"
      ],
      "xProperty": "time",
      "title": "Velocity of Star, Relative to Observer",
      "titlePosition": "left",
      "buttonsLayout": "horizontal",
      "buttonsStyle": "text",
      "lineWidth": 2,
      "xlabel": "Time (days)",
      "xmin": 0,
      "xmax": 600,
      "ylabel": "Velocity (m/s)",
      "ymin": -2.5,
      "ymax": 2.5,
      "xTickCount": 8,
      "yTickCount": 4,
      "yFormatter": ".2s",
      "dataColors": [
        "#0000ff"
      ],
      "syncXAxis": "light-intensity"
    },
    {
      "id": "light-intensity",
      "type": "graph",
      "enableAxisScaling": false,
      "enableZooming": false,
      "properties": [
        "lightIntensity"
      ],
      "xProperty": "time",
      "title": "Light Intensity of Star",
      "titlePosition": "left",
      "buttonsLayout": "horizontal",
      "buttonsStyle": "text",
      "lineWidth": 2,
      "xlabel": "Time (days)",
      "xmin": 0,
      "xmax": 600,
      "ylabel": "Light Intensity",
      "ymin": 0.7,
      "ymax": 1.02,
      "xTickCount": 8,
      "yTickCount": 4,
      "dataColors": [
        "#0000ff"
      ],
      "syncXAxis": "star-cam-velocity"
    },
    {
      "id": "hab-star-type",
      "type": "numericOutput",
      "property": "habitability.starType",
      "label": "Star type:",
      "displayValue": "return value != null ? (value ? '✓' : '✗') : '?'"
    },
    {
      "id": "hab-planet-type",
      "type": "numericOutput",
      "property": "habitability.planetType",
      "label": "Planet type:",
      "displayValue": "return value != null ? (value ? '✓' : '✗') : '?'"
    },
    {
      "id": "hab-planet-size",
      "type": "numericOutput",
      "property": "habitability.planetSize",
      "label": "Planet size:",
      "displayValue": "return value != null ? (value ? '✓' : '✗') : '?'"
    },
    {
      "id": "hab-orbital-distance",
      "type": "numericOutput",
      "property": "habitability.orbitalDistance",
      "label": "Orbital distance:",
      "displayValue": "return value != null ? (value ? '✓' : '✗') : '?'"
    },
    {
      "id": "playback",
      "type": "playback",
      "stepping": true
    }
  ],
  "layout": {
    "playback-container": [
      "playback"
    ],
    "playback-right": [
      "timestep"
    ],
    "bottom-1": [
      ["tilt-locked"],
      ["habitation-zone-vis"],
      ["preset-star"],
      ["preset-planet"],
      ["planet-rocky"],
      ["planet-diameter", "planet-mass"],
      ["telescope-precision"],
      ["circular-orbit"]
    ],
    "habitability": [
      ["habitability-label", "analyze"],
      ["hab-star-type"],
      ["hab-planet-type"],
      ["hab-planet-size"],
      ["hab-orbital-distance"]
    ],
    "right-top": [
      "star-cam-velocity"
    ],
    "right-bottom": [
      "light-intensity"
    ],
    "under-graphs": [
      "clear-graphs"
    ]
  },
  "template": [
    {
      "id": "playback-container",
      "top": "model.bottom",
      "left": "model.left",
      "padding-top": "0.75em",
      "padding-right": "0.5em"
    },
    {
      "id": "playback-right",
      "top": "model.bottom",
      "left": "playback-container.right"
    },
    {
      "id": "bottom-1",
      "left": "model.left",
      "top": "playback-right.bottom",
      "margin-top": "1em"
    },
    {
      "id": "habitability",
      "left": "model.left",
      "top": "bottom-1.bottom",
      "margin-top": "1em",
      "border": "1px solid #bbb"
    },
    {
      "id": "right-top",
      "left": "model.right",
      "height": "interactive.height * 0.46",
      "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    },
    {
      "id": "right-bottom",
      "left": "model.right",
      "top": "right-top.bottom",
      "height": "interactive.height * 0.46",
      "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    },
    {
      "id": "under-graphs",
      "left": "right-bottom.left",
      "right": "right-bottom.right",
      "top": "right-bottom.bottom",
      "align": "right",
      "margin-right": "1em",
      "margin-top": "1em"
    }
  ]
}
